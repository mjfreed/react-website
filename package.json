{
    "name": "react-website",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "start": "react-scripts start",
        "compile": "tsc deployment/deployBuild.ts --outDir ./build",
        "build": "webpack --mode production",
        "deploy": "ts-node deployment/deployBucket.ts",
        "upload-client": "aws s3 sync dist/ s3://mfreed-website-bucket",
        "test": "react-scripts test",
        "eject": "react-scripts eject",
        "prepare": "husky install",
        "postinstall": "semantic-ui-css-patch",
        "pretest": "cp tsconfig.json tsconfig.json.save",
        "posttest": "mv tsconfig.json.save tsconfig.json"
    },
    "dependencies": {
        "aws-sdk": "^2.1083.0",
        "react": "^17.0.2",
        "react-dom": "^17.0.2",
        "react-router-dom": "^5.3.0",
        "react-scripts": "4.0.3",
        "semantic-ui-css": "^2.4.1",
        "semantic-ui-react": "^2.0.4",
        "typescript": "^4.4.3",
        "uuid": "^8.3.2",
        "web-vitals": "^1.1.2"
    },
    "devDependencies": {
        "@semantic-ui-react/css-patch": "^1.0.0",
        "@testing-library/jest-dom": "^5.16.1",
        "@testing-library/react": "^12.1.2",
        "@testing-library/user-event": "^13.5.0",
        "@types/jest": "^26.0.24",
        "@types/node": "^12.20.26",
        "@types/react": "^17.0.37",
        "@types/react-dom": "^17.0.9",
        "@types/react-router-dom": "^5.3.0",
        "@types/react-transition-group": "^4.4.3",
        "@types/semantic-ui": "^2.2.7",
        "@types/styletron-engine-atomic": "^1.1.1",
        "@types/styletron-react": "^5.0.3",
        "@types/uuid": "^8.3.3",
        "css-loader": "^6.6.0",
        "html-loader": "^3.1.0",
        "html-webpack-plugin": "^5.5.0",
        "husky": "^7.0.2",
        "lint-staged": "^11.1.2",
        "prettier": "^2.4.1",
        "style-loader": "^3.3.1",
        "ts-loader": "^9.2.6",
        "webpack": "^5.68.0",
        "webpack-cli": "^4.9.2"
    },
    "eslintConfig": {
        "extends": [
            "react-app",
            "react-app/jest"
        ]
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "lint-staged": {
        "**/*": "prettier --write --ignore-unknown"
    }
}
